{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "/favicon.ico",
      "/robots.txt",
      "/api/*",
      "/_next/*",
      "/static/*"
    ]
  },
  "routes": [
    {
      "route": "/api/*",
      "rewrite": "/api-func/api/$1"
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/_not-found",
      "statusCode": 404
    }
  }
}

